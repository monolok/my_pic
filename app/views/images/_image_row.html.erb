<div class="col-xs-offset-1 col-xs-4 col-xs-offset-1">
	<div class="mark-name"><%= image.name%></div>
	<%=link_to image_tag(url_for(:controller => 'images', :action => '_show_image', :id => image.id), class: "pad-bot1 img-rounded"), image %> 
	<%if image.active == false%>
		<div class="text-muted mark">
			<i class="fa fa-copyright"></i><strong>Copyright not available</strong>	
		</div>
	<%else%>
		<div class="text-success mark">
			<i class="fa fa-copyright"></i><strong>Copyright available</strong>	
		</div>
	<%end%>
</div>	
